{
  "name": "perl-it-online",
  "version": "0.0.1",
  "description": "Perl - in browser (WebPerl)",
  "scripts": {
    "build": "npm run prettier:format && npm run build:esbuild",
    "build:watch": "npm run prettier:format && npm run build:esbuild:watch",
    "build:esbuild": "node esbuild.js prod",
    "build:esbuild:watch": "node esbuild.js dev watch",
    "prettier:check": "prettier --config .prettierrc 'js/**/*.ts' 'js/langs/**/*.js' esbuild.js",
    "prettier:format": "prettier --config .prettierrc 'js/**/*.ts' esbuild.js --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dom111/perl-it-online.git"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/dom111/perl-it-online/issues"
  },
  "homepage": "https://github.com/dom111/perl-it-online#readme",
  "dependencies": {
    "@gfx/zopfli": "^1.0.15",
    "@types/codemirror": "^5.60.5",
    "codemirror": "^5.65.0",
    "esbuild": "^0.14.10",
    "esbuild-sass-plugin": "^2.0.0",
    "prettier": "^2.5.1",
    "xterm": "^4.16.0",
    "xterm-addon-fit": "^0.5.0"
  }
}
